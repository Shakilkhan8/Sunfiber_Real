<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

       <!-- Inherit Form View to Modify it -->
       <record id="invoice_line_inherit_view_id" model="ir.ui.view">
           <field name="name">Invoice Line</field>
           <field name="model">account.move</field>
           <field name="inherit_id" ref="account.view_move_form"/>
           <field name="arch" type="xml">

               <xpath expr="//form/sheet/notebook/page/field[@name='invoice_line_ids']/tree/field[@name='account_id']" position="after">
                   <!-- Add your fields or attributes here -->
                   <field name="quality_id"/>
                   <field name="sqf"/>
                   <field name="discount"/>
               </xpath>

           </field>
       </record>

    </data>
</odoo>